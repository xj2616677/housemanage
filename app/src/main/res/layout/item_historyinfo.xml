<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">



    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/nametext_itemhistory"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="15dp"
            android:textSize="20sp"
            android:textColor="@color/red"/>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/jgdz"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text1_itemhistory"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/ssrw"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text2_itemhistory"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/rwlx"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text3_itemhistory"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/jcrq"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text4_itemhistory"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/jcr"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text5_itemhistory"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="5"
                android:textSize="20sp"/>
        </LinearLayout>
        <!--<TextView-->
            <!--android:id="@+id/text_history_endchecked"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:textSize="20sp"-->
            <!--android:layout_marginBottom="15dp"-->
            <!--android:layout_marginLeft="20dp"-->
            <!--android:text="检查已结束"-->
            <!--android:textColor="@color/red"-->
            <!--/>-->

        <LinearLayout
            android:id="@+id/lin_itemhistory_inspectno"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:visibility="gone"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="本系统检查单号:"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text_itemhistory_inspectno"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:textSize="20sp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lin_itemhistory_state"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:visibility="gone"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上传市系统状态:"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text_itemhistory_state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:textColor="@color/red"
                android:textSize="20sp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_itemhistory_result"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:visibility="gone"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/text_itemhistory_resulttitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="市系统检查单号:"
                android:layout_marginLeft="20dp"
                android:textSize="20sp"/>
            <TextView
                android:id="@+id/text_itemhistory_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:textColor="@color/red"
                android:textSize="20sp"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        >
        <TextView
            android:id="@+id/text_historyitem"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:padding="5dp"
            android:gravity="center"
            android:textColor="@color/white"
            android:background="@drawable/button_color_selector"

            />

        <TextView
            android:id="@+id/text_historyitem_feed"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:padding="5dp"
            android:gravity="center"
            android:layout_marginTop="20dp"
            android:text="反馈"
            android:textColor="@color/white"
            android:background="@drawable/button_color_selector"
            />

    </LinearLayout>


</RelativeLayout>